{
  "name": "nutriv",
  "version": "1.0.0",
  "description": "Restaurant with home delivery!",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "electron-start": "electron main.js",
    "electron-package": "electron-forge package",
    "electron-make": "electron-forge make",
    "electron-publish": "electron-forge publish",
    "electron-lint": "echo \"No linting configured\"",
    "electron-build-installer": "electron-builder"
  },
  "build": {
    "appId": "nutriv",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/img/favicon.ico"
    },
    "nsis": {
      "installerIcon": "public/img/favicon.ico",
      "uninstallerIcon": "public/img/favicon.ico",
      "uninstallDisplayName": "Nutriv",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Nutriv Corporation",
  "license": "ISC",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "Akhil"
        }
      ]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap-icons": "^1.7.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.5.1",
    "mongoose": "^6.0.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.0",
    "os-utils": "0.0.14",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.29.2",
    "slugify": "^1.6.1",
    "stripe": "^8.186.1",
    "validator": "^13.6.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "electron": "^16.0.0",
    "electron-builder": "^22.13.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier": "^2.4.1"
  }
}
